{
  "name": "lucia",
  "version": "0.6.3",
  "description": "3kb library for tiny web apps",
  "main": "dist/lucia.umd.js",
  "module": "dist/lucia.esm.js",
  "browser": "dist/lucia.min.js",
  "types": "dist/lucia.d.ts",
  "scripts": {
    "dev": "sh scripts/dev.sh",
    "build": "sh scripts/build.sh",
    "docs": "typedoc src/index.ts",
    "lint": "eslint src/**",
    "cleanup": "prettier --write src --parser typescript",
    "test": "jest --silent --runInBand",
    "release": "sh scripts/release.sh",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aidenybai/lucia.git"
  },
  "exports": {
    "browser": "./dist/lucia.min.js",
    "require": "./dist/lucia.cjs.js",
    "import": "./dist/lucia.esm.js"
  },
  "sideEffects": false,
  "keywords": [
    "lucia",
    "lucia.js",
    "tiny",
    "javascript",
    "library",
    "web",
    "ui",
    "browser"
  ],
  "author": "Aiden Bai <hello@aidenybai.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aidenybai/lucia/issues"
  },
  "homepage": "https://lucia.js.org",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-strip": "^2.0.0",
    "@testing-library/dom": "^7.28.1",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@wessberg/rollup-plugin-ts": "^1.3.14",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.1.2",
    "rollup": "2.50.5",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.2",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4",
    "vite": "^2.3.3"
  }
}
